<!DOCTYPE html>
<html>
<head>
  <title>Products</title>
  <link rel="stylesheet" type="text/css" href="/styles.css">
</head>
<body>
  <h1>Our Products</h1>
  <div class="product-list">
    <% products.forEach(product => { %>
      <div class="product-card">
        <h2><%= product.name %></h2>
        <p><%= product.price %></p>
        <a href="/products/<%= product.id %>">View Details</a>
      </div>
    <% }) %>
  </div>
  <a href="/">Go Back Home</a>
</body>
</html>
